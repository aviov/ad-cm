{
  "name": "core-api",
  "version": "1.0.0",
  "description": "express server for campaign mamagement web application",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aviov/ad-cm.git"
  },
  "keywords": [
    "api",
    "backend"
  ],
  "author": "Juri.L",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/aviov/ad-cm/issues"
  },
  "scripts": {
    "test": "jest",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "npm run typeorm -- migration:generate -d src/database/data-source.ts",
    "migration:run": "npm run typeorm -- migration:run -d src/database/data-source.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d src/database/data-source.ts",
    "migration:generate:docker": "docker exec -it ad-cm-core-api-1 npm run migration:generate",
    "migration:run:docker": "docker exec -it ad-cm-core-api-1 npm run migration:run",
    "migration:revert:docker": "docker exec -it ad-cm-core-api-1 npm run migration:revert",
    "seed": "ts-node src/database/seeds/index.ts",
    "seed:docker": "docker exec -it ad-cm-core-api-1 npm run seed"
  },
  "homepage": "https://github.com/aviov/ad-cm#readme",
  "dependencies": {
    "axios": "^1.8.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.5",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
